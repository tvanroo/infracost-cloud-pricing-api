{
  "name": "cloud-pricing-api",
  "version": "0.3.7",
  "description": "Cloud Pricing API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "postbuild": "copyfiles -u 1 src/views/**/** dist && copyfiles -u 1 src/public/**/** dist",
    "start": "node dist/server.js",
    "db:setup": "node dist/cmd/dbSetup.js",
    "data:scrape": "node dist/cmd/dataScrape.js",
    "data:dump": "node dist/cmd/dataDump.js",
    "data:download": "node dist/cmd/dataDownload.js",
    "data:load": "node dist/cmd/dataLoad.js",
    "data:status": "node dist/cmd/dataStatus.js",
    "job:init": "npm run db:setup && npm run job:update",
    "job:update": "npm run data:download && npm run data:load",
    "db:setup:dev": "ts-node src/cmd/dbSetup.ts",
    "data:scrape:dev": "ts-node src/cmd/dataScrape.ts",
    "data:dump:dev": "ts-node src/cmd/dataDump.ts",
    "data:download:dev": "ts-node src/cmd/dataDownload.ts",
    "data:load:dev": "ts-node src/cmd/dataLoad.ts",
    "data:status:dev": "ts-node src/cmd/dataStatus.ts",
    "job:init:dev": "npm run db:setup:dev && npm run job:update:dev",
    "job:update:dev": "npm run data:download:dev && npm run data:load:dev",
    "lint": "eslint --ext .ts,.js --ignore-path .gitignore .",
    "format": "prettier --write 'src/**/*.{js,ts}'"
  },
  "author": "Infracost",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://raw.githubusercontent.com/infracost/cloud-pricing-api/master/LICENSE"
    }
  ],
  "dependencies": {
    "@graphql-tools/schema": "^8.3.2",
    "@graphql-tools/utils": "^8.12.0",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.6.7",
    "apollo-server-plugin-base": "^3.5.1",
    "async-mutex": "^0.3.2",
    "axios": "^0.27.2",
    "decimal.js": "^10.3.1",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.8",
    "express": "^4.17.2",
    "glob": "^7.1.7",
    "googleapis": "^100.0.0",
    "graphql": "^16.3.0",
    "lodash": "^4.17.21",
    "mingo": "^6.1.0",
    "node-cache": "^5.1.2",
    "pg": "^8.7.3",
    "pg-copy-streams": "^6.0.2",
    "pg-format": "^1.0.4",
    "pino": "^6.14.0",
    "pino-http": "~6.0.0",
    "pino-pretty": "^6.0.0",
    "prettier": "^2.7.1",
    "progress": "^2.0.3",
    "tmp": "^0.2.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.17.10",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.23",
    "@types/pg": "^8.6.1",
    "@types/pg-copy-streams": "^1.2.1",
    "@types/pg-format": "^1.0.2",
    "@types/pino-http": "^5.8.1",
    "@types/prettier": "^2.7.0",
    "@types/progress": "^2.0.5",
    "@types/tmp": "^0.2.1",
    "@types/yargs": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.37.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infracost/cloud-pricing-api.git"
  }
}
